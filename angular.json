{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "style": "scss",
      "skipTests": true,
      "changeDetection": "OnPush"
    }
  },
  "defaultProject": "angular-template",
  "projects": {
    "angular-template": {
      "projectType": "application",
      "root": "apps/angular-template",
      "sourceRoot": "apps/angular-template/src",
      "prefix": "mr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/angular-template",
            "index": "apps/angular-template/src/index.html",
            "main": "apps/angular-template/src/main.ts",
            "polyfills": "apps/angular-template/src/polyfills.ts",
            "tsConfig": "apps/angular-template/tsconfig.app.json",
            "assets": [
              "apps/angular-template/src/favicon.ico",
              "apps/angular-template/src/assets",
              {
                "input": "./libs/assets/",
                "glob": "**/*",
                "output": "./assets"
              }
            ],
            "styles": ["apps/angular-template/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular-template/src/environments/environment.ts",
                  "with": "apps/angular-template/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-template:build:production"
            },
            "development": {
              "browserTarget": "angular-template:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-template:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/angular-template/src/**/*.ts",
              "apps/angular-template/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular-template"],
          "options": {
            "jestConfig": "apps/angular-template/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "assets": {
      "projectType": "library",
      "root": "libs/assets",
      "sourceRoot": "libs/assets/src"
    },
    "image": {
      "projectType": "library",
      "root": "libs/image",
      "sourceRoot": "libs/image/src",
      "prefix": "mr",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/image"],
          "options": {
            "jestConfig": "libs/image/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/image/src/**/*.ts",
              "libs/image/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-styles": {
      "projectType": "library",
      "root": "libs/shared-styles",
      "sourceRoot": "libs/shared-styles/src"
    }
  }
}
